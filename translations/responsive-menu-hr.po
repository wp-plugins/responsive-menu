#: classes/class.responsiveMenu.php:39 classes/class.responsiveMenu.php:352
msgid "Menu Title"
msgstr "Naslov izbornika"

#: classes/class.responsiveMenu.php:93 classes/class.responsiveMenu.php:94
msgid "Responsive Menu"
msgstr "Responsive Menu"

#: classes/class.responsiveMenu.php:295
msgid "Responsive Menu Options"
msgstr "Mogućnosti Responsive Menu-a"

#: classes/class.responsiveMenu.php:313
msgid "Initial Checks"
msgstr "Inicijalne provjere"

#: classes/class.responsiveMenu.php:315
msgid "Viewport Meta Tag Check"
msgstr "Provjera Viewport Meta Tag-a"

#: classes/class.responsiveMenu.php:319
